{
  "name": "mcstatic",
  "version": "0.0.1",
  "description": "A custom server for Minecraft Classic and Classicube, written in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "test": "exit 0",
    "start": "node dist/index.js",
    "dev": "nodemon --watch dist -e js dist/index.js",
    "build": "tsc",
    "build:watch": "npm run build -- -w",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./src/**/*.ts ./.eslintrc.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/727021/MCStatic.git"
  },
  "author": "727021",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/727021/MCStatic/issues"
  },
  "homepage": "https://github.com/727021/MCStatic#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/debug": "^4.1.8",
    "@types/eslint": "^8.40.2",
    "@types/node": "^20.3.2",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "randomstring": "^1.3.0"
  }
}
